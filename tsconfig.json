{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/register.tsx",
    "pages/login.tsx"
, "pages/post/new/index.tsx", "pages/home.tsx", "layouts/Cardpost.tsx", "layouts/api/auth/SignUp.tsx", "pages/profile/index.tsx", "pages/post/[postId].jsx", "layouts/components/button/loginbutton.tsx", "pages/post/title/[title].jsx", "pages/edit.jsx", "layouts/components/MuiNavbar.jsx", "layouts/components/hooks/useGetPosts.jsx", "layouts/components/ui/Changetab.jsx", "pages/profile/[userid].jsx", "pages/profile/edit/email.tsx", "pages/ninjas/[id].tsx", "pages/posts/[id].js"  ],
  "exclude": [
    "node_modules"
  ]
}
