{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/register/index.tsx",
    "pages/login/index.tsx",
    "pages/post/new/index.tsx",
    "pages/home.tsx",
    "layouts/components/CardPost.tsxardpost.tsx",
    "layouts/api/auth/SignUp.tsx",
    "pages/profile/index.tsx",
    "pages/post/[postId].jsx",
    "layouts/components/button/LoginButton.tsx",
    "pages/post/title/[title].jsx",
    "pages/edit.jsx",
    "layouts/components/MuiNavbar.jsx",
    "layouts/components/hooks/useFetchPosts.tsx",
    "layouts/components/ui/Changetab.jsx",
    "pages/profile/[userid].tsx",
    "pages/profile/edit/email.tsx",
    "pages/ninjas/[id].tsx",
    "pages/posts/[id].js",
    "pages/post/categories/[id].tsx",
    "layouts/context/AuthContext.js",
    "pages/post/[id].tsx",
    "layouts/components/ui/CardPost.jsx",
    "layouts/components/ui/CardPost.jsx",
    "pages/post/edit/[id].tsx"
  ],
  "exclude": ["node_modules"]
}
